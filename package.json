{
  "name": "angular2-seed",
  "version": "0.2.0",
  "description": "Angular 2 seed",
  "scripts": {
    "prebuild": "npm install && npm run clean",
    "clean": "rimraf coverage dist",

    "test": "karma start --single-run",
    "watch:test": "karma start",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/report.json -o coverage/html-report -t html",
    "coverage:report": "istanbul report",
    "build": "npm run build:app",
    "build:app": "webpack --config config/webpack.config.js --progress --profile",
    "build:prod": "set NODE_ENV=production && npm run build",
    "build:staging": "set NODE_ENV=staging && npm run build",
    "watch:dev": "npm run build -- --watch",
    "watch": "npm run watch:dev",
    "webpack": "webpack"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meeroslav/angular2-seed.git"
  },
  "keywords": [
    "angular2",
    "seed",
    "webpack",
    "typescript",
    "SPA",
    "ng2-translate"
  ],
  "author": "Miroslav Jonas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meeroslav/angular2-seed/issues"
  },
  "homepage": "https://github.com/meeroslav/angular2-seed#readme",
  "devDependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@angular/router": "3.2.1",
    "@angular/forms": "0.2.0",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.2.34",
    "@types/jquery": "^2.0.34",
    "@types/node": "^6.0.38",
    "@types/selenium-webdriver": "2.53.33",
    "@types/source-map": "^0.1.27",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^1.12.34",
    "awesome-typescript-loader": "^2.2.1",
    "angular2-template-loader": "^0.6.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.26.0",
    "expose-loader": "^0.7.1",
    "extendify": "^1.0.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "ie-shim": "^0.1.0",
    "jasmine-core": "^2.4.1",
    "jquery": "^3.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-coverage": "^0.1.1",
    "karma-webpack": "1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "ng2-translate": "^4.0.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "~2.5.4",
    "rxjs": "5.0.0-beta.12",
    "style-loader": "^0.13.0",
    "ts-helpers": "1.1.2",
    "ts-node": "^1.7.0",
    "tslint": "~3.15.1",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.5.0",
    "typescript": "^2.0.6",
    "url-loader": "^0.5.7",
