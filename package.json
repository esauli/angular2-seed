    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "~1.0.1",
    "webpack-dev-server": "^2.1.0-beta.2",
    "zone.js": "~0.6.26"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
